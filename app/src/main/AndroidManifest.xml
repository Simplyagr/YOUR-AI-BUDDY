        <activity
            android:name=".FourthActivity"
            android:exported="true"
            android:label="My Goals"
            android:theme="@style/Theme.NewNavigation.NoActionBar" />
        <activity
            android:name=".FifthActivity"
            android:exported="true"
            android:label="FAQs"
            android:theme="@style/Theme.NewNavigation.NoActionBar" />
        <activity
            android:name=".onboardingOne.OnBoardingDesignOne"
            android:exported="true"
            android:label="How To Use"
            android:theme="@style/Theme.NewNavigation.NoActionBar" />
        <activity
            android:name=".AboutUs"
            android:exported="true"
            android:label="About Us"
            android:theme="@style/Theme.NewNavigation.NoActionBar" />
    </application>

</manifest>